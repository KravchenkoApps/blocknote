<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/imageZam_dark"
        android:layout_width="37dp"
        android:layout_height="39dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="22dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_menu_book_white_48" />

    <ImageView
        android:id="@+id/imageSpis_dark"
        android:layout_width="37dp"
        android:layout_height="36dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="28dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageZam_dark"
        app:srcCompat="@drawable/baseline_format_list_bulleted_white_48" />

    <TextView
        android:id="@+id/zam_dark"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="27dp"
        android:layout_marginRight="32dp"
        android:clickable="true"
        android:contextClickable="false"
        android:text="@string/note1"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageZam_dark"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/spis_dark"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="40dp"
        android:layout_marginRight="32dp"
        android:clickable="true"
        android:contextClickable="false"
        android:text="@string/list1"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageSpis_dark"
        app:layout_constraintTop_toBottomOf="@+id/zam_dark" />

</androidx.constraintlayout.widget.ConstraintLayout>